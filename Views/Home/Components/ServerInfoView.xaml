<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:type="using:SteveLauncher.Domain.Entity"
             x:Class="SteveLauncher.Views.Home.Components.ServerInfoView"
             x:DataType="type:MinecraftServerInfo">
    <VerticalStackLayout>
        <Label Text="{Binding HostName.HostName}"/>
        <BoxView StyleClass="Divider"/>
        <Label Text="{Binding Icon}"></Label>
    </VerticalStackLayout>
</ContentView>